	<queryString>
		<![CDATA[SELECT
fe_facturas.id,
c_clientes.identificacion,
c_clientes.nombre_completo AS receptor_nombre,
c_clientes.tipo_de_identificacion_id AS receptor_tipo_identif,
c_clientes.identificacion AS receptor_num_identif,

CONCAT('+(',c_clientes.codigo_pais,') ',c_clientes.fax) AS receptor_fax_b,
CONCAT('+(',c_clientes.codigo_pais,') ',c_clientes.telefono1) AS receptor_tel_b,

(
CONCAT((SELECT 
CONCAT(c_provincias.provincia,', ',c_cantones.canton,', ',c_distritos.distrito,', ',c_barrios.barrio)
FROM
c_provincias
INNER JOIN c_cantones ON c_cantones.provincia_id = c_provincias.id
INNER JOIN c_distritos ON c_distritos.canton_id = c_cantones.id
INNER JOIN c_barrios ON c_barrios.distrito_id = c_distritos.id
WHERE c_provincias.id = c_clientes.provincia_id AND
c_cantones.id = c_clientes.canton_id AND c_distritos.id = c_clientes.distrito_id AND
c_barrios.id = c_clientes.barrio_id),' , ',c_clientes.otras_senas)
) AS direccion_receptor,
fe_facturas.tipo_documento,
fe_facturas.otros,
c_clientes.codigo_pais AS receptor_cod_pais_fax,
c_clientes.codigo_pais AS receptor_cod_pais_tel,
c_clientes.correo1 AS receptor_email,
c_clientes.fax AS receptor_fax_a,
c_clientes.telefono1 AS receptor_tel_a,
fe_facturas.tipo_cambio,
fe_facturas.total_comprobante,
fe_facturas.total_descuentos,
fe_facturas.total_venta_neta,
fe_facturas.total_exentos,
fe_facturas.total_gravados,
fe_facturas.total_imp,
fe_facturas.total_merc_exentas,
fe_facturas.total_merc_gravadas,
fe_facturas.total_serv_exentos,
fe_facturas.total_serv_gravados,
fe_facturas.total_ventas,
fe_facturas.clave,
fe_facturas.consecutivo,
emisores.fax AS emisor_fax_a,
emisores.telefono AS emisor_tel_a,
CONCAT( '+(', emisores.codigo_pais, ') ', emisores.fax ) AS emisor_fax_b,
CONCAT( '+(', emisores.codigo_pais, ') ', emisores.telefono ) AS emisor_tel_b,
emisores.email AS emisor_email,
emisores.nombre_razon_social AS emisor_nombre,
emisores.identificacion AS emisor_num_identif,
emisores.tipo_de_identificacion_id AS emisor_tipo_identif,
DATE_FORMAT( fe_facturas.fecha_emision, '%d/%m/%Y - %r' ) AS fecha_emision,
emisores.nombre_legal_empresa AS receptor_tel_anombre_comercial,
fe_facturas.numero_factura,
IFNULL( fe_facturas.total_exonerado, 0.00 ) AS total_exonerado,
IFNULL( fe_facturas.total_iva_devuelto, 0.00 ) AS total_iva_devuelto,
IFNULL( fe_facturas.total_merc_exonerada, 0.00 ) AS total_merc_exonerada,
IFNULL( fe_facturas.total_otros_cargos, 0.00 ) AS total_otros_cargos,
IFNULL( fe_facturas.total_serv_exonerado, 0.00 ) AS total_serv_exonerado,
emisores.detalle_en_factura1,
emisores.detalle_en_factura2,
CONCAT(c_provincias.provincia,', ',
		c_cantones.canton,
		', ',
		c_distritos.distrito,
		', ',
		IFNULL(c_barrios.barrio,''),
		', ',
		emisores.otras_senas 
	) AS direccion_emisor,
c_provincias.provincia,
c_cantones.canton,
c_distritos.distrito,
c_barrios.barrio,
IFNULL(c_monedas.simbolo_moneda,'CRC') AS cod_moneda
FROM
fe_facturas
LEFT JOIN emisores ON emisores.id = fe_facturas.emisor_id
LEFT JOIN c_clientes ON c_clientes.id = fe_facturas.cliente_id
LEFT JOIN c_provincias ON c_provincias.id = emisores.provincia_id
LEFT JOIN c_cantones ON c_cantones.id = emisores.canton_id
LEFT JOIN c_distritos ON c_distritos.id = emisores.distrito_id AND c_distritos.id = emisores.distrito_id
LEFT JOIN c_barrios ON c_barrios.id = emisores.barrio_id AND c_barrios.id = emisores.barrio_id
INNER JOIN c_monedas ON fe_facturas.moneda_id = c_monedas.id
WHERE
fe_facturas.id = $P{CLAVE_FACTURA}]]>
	</queryString>
